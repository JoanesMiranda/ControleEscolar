	<queryString language="SQL">
		<![CDATA[SELECT
     aluno.`idAluno` AS aluno_idAluno,
     aluno.`nome` AS aluno_nome,
     aluno.`matricula` AS aluno_matricula,
     aluno.`codcartaoarduino` AS aluno_codcartaoarduino,
     chamada.`idChamada` AS chamada_idChamada,
     chamada.`faltas` AS chamada_faltas,
     chamada.`data` AS chamada_data,
     chamada.`FK_Professor` AS chamada_FK_Professor,
     chamada.`FK_Disciplina` AS chamada_FK_Disciplina,
     chamada.`FK_Aluno` AS chamada_FK_Aluno,
     professor.`idProfessor` AS professor_idProfessor,
     professor.`nome` AS professor_nome,
     professor.`matricula` AS professor_matricula,
     turma.`idTurma` AS turma_idTurma,
     turma.`nome` AS turma_nome,
     turma.`codigo` AS turma_codigo,
     turma_has_professor.`FK_Turma` AS turma_has_professor_FK_Turma,
     turma_has_professor.`FK_Professor` AS turma_has_professor_FK_Professor
FROM
     `aluno` aluno INNER JOIN `chamada` chamada ON aluno.`idAluno` = chamada.`FK_Aluno`
     INNER JOIN `professor` professor ON chamada.`FK_Professor` = professor.`idProfessor`
     INNER JOIN `turma_has_professor` turma_has_professor ON professor.`idProfessor` = turma_has_professor.`FK_Professor`
     INNER JOIN `turma` turma ON turma_has_professor.`FK_Turma` = turma.`idTurma`]]>
	</queryString>
